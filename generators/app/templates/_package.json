{
  "name": "<%= moduleName %>",
  "version": "0.0.0",
  "description": "<%= moduleDescription %>",
  "license": "MIT",
  "repository": "<%= githubUsername %>/<%= repoName %>",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },<% if (cli) { %>
  "bin": "cli.js",<% } %>
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "npm run lint && <% if (nyc) { %>nyc <% } %>ava -v",
    "lint": "prettier --single-quote --write *.js"
  },
  "files": [
    "index.js"<% if (cli) { %>,
    "cli.js"<% } %>
  ],
  "keywords": [<% if (cli) { %>
    "cli-app",
    "cli",<% } %>
    ""
  ],
  "dependencies": {},
  "devDependencies": {
    "ava": "0.25.0",<% if (codecov) { %>
    "codecov": "3.1.0",<% } %><% if (nyc) { %>
    "nyc": "13.0.1",<% } %>
    "prettier": "*"
  }<% if (codecov) { %>,
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  }<% } %>
}
